{
  "name": "web",
  "private": true,
  "homepage": "https://lettertothefuture.github.io/web/",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -b && vite build",
    "format": "prettier --write .",
    "lint": "eslint . --max-warnings 0",
    "lint:staged": "lint-staged",
    "test": "vitest --ui --config vite.config.test.ts",
    "unit:check": "vitest run --config vite.config.test.ts",
    "commit-message": "validate-commit-msg",
    "start": "vite",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint . --fix --max-warnings 0",
      "pnpm format"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^5.64.1",
    "date-fns": "^4.1.0",
    "gh-pages": "^6.3.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-jss": "^10.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/lodash": "^4.17.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jss": "^10.10.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  },
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321"
}
